{
    "name": "mucts/laravel-sms",
    "description": "SMS SDK for Laravel 7",
    "type": "library",
    "require": {
        "php": "^7.1",
        "laravel/framework": "^7.10",
        "mucts/sms": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "MuCTS\\LaravelSMS\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "herry",
            "email": "yuandeng@aliyun.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "MuCTS\\LaravelSMS\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "SMS": "MuCTS\\LaravelSMS\\Facades\\SMS"
            }
        }
    }
}
